  <!-- Leader board -->
  <div class="px-4 py-5 my-5 text-center">
    <div id="main">
      <div id="header">
        <h1>Leaderboard</h1>
      </div>
      <div id="leaderboard">
        <div class="ribbon "></div>
        <table>
          <% users.forEach(function(user, index) { %>
            <tr>
              <td class="number">
                <%= index + 1 %>
              </td>
              <td class="profile-image">
                <img src="./images/<%= user._id %>" width="50" height="50" alt="Profile" />
              </td>
              <td class="name">
                <%= user.username %>
              </td>
              <td class="points">
                <%= user.points %>
                  <% if (index===0) { %>
                    <img class="gold-medal"
                      src="https://github.com/malunaridev/Challenges-iCodeThis/blob/master/4-leaderboard/assets/gold-medal.png?raw=true"
                      alt="gold medal" />
                    <% } %>
              </td>
            </tr>
            <% }); %>
        </table>
        <div class="firework" id="firework1" style="top: 20%; left: 20%;"></div>
        <div class="firework" id="firework2" style="top: 40%; left: 40%;"></div>
      </div>
    </div>
  </div>

